{
    "name": "digital-creative/ecs",
    "description": "Standard code style configuration",
    "require": {
        "php": ">=8.0",
        "symplify/easy-coding-standard": "^11.3.4"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\ECS\\": "src/"
        },
        "files": [
            "helpers/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
